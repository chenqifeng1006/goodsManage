<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.qinziwang.domain">

	<class name="Photo" table="photo">
		<id name="id" type="int" length="20">
			<generator class="identity"/>
		</id>
		
		 <property
			name="name"
			column="name" 
			type="string" 
			length="20"
		/>
			 <property
			name="username"
			column="username" 
			type="string" 
			length="20"
		/>
		
	</class>
	
</hibernate-mapping>
