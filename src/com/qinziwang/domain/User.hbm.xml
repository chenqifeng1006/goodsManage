<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.qinziwang.domain">

	<class name="User" table="user">
		<id name="username" type="string" length="20">
			<generator class="assigned"/>
		</id>
		
		 <property
			name="password"
			column="password" 
			type="string" 
			length="20"
		/>
			 <property
			name="phone"
			column="phone" 
			type="string" 
			length="18"
		/>
			 <property
			name="address"
			column="address" 
			type="string" 
			length="50"
		/>

	</class>
	
</hibernate-mapping>
