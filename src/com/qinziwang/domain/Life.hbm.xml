<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.qinziwang.domain">

	<class name="Life" table="life">
		<id name="id" type="int" length="20">
			<generator class="identity"/>
		</id>
		
		 <property
			name="name"
			column="name" 
			type="string" 
			length="20"
		/>
		<property
			name="brand"
			column="brand" 
			type="string" 
			length="20"
		/>
		 <property
			name="color"
			column="color" 
			type="string" 
			length="50"
		/>
			 <property
			name="price"
			column="price" 
			type="double" 
			length="50"
		/>			
		 <property
			name="fileid"
			column="fileid" 
			type="string" 
			length="50"
		/>
	</class>
	
</hibernate-mapping>
