<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.qinziwang.domain">

	<class name="FilePicture" table="file">
		<id name="id" type="int" length="20">
			<generator class="identity"/>
		</id>
		
		 <property
			name="src"
			column="src" 
			type="string" 
			length="200"
		/>

		 <property
			name="photoid"
			column="photoid" 
			type="int" 
			length="50"
		/>

	</class>
	
</hibernate-mapping>
