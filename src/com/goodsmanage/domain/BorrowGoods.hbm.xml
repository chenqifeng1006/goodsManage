<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.goodsmanage.domain">

	<class name="BorrowGoods" table="borrow_goods">
		<id name="id" type="int" length="16">
			<generator class="identity"/>
		</id>
		<property
			name="goodsname"
			column="goodsname" 
			type="string" 
			length="20"
		/>
		 <property
			name="goodsno"
			column="goodsno" 
			type="string" 
			length="16"
		/>
			 <property
			name="status"
			column="status" 
			type="string" 
			length="10"
		/>


	</class>
	
</hibernate-mapping>
